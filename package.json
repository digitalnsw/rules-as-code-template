{
	"name": "design-system-template",
	"version": "1.1.1",
	"description": "The NSW Government Design System Template",
	"author": "Damian Dzieduch <damian@codeforaustralia.org",
	"repository": {
		"type": "git",
		"url": "https://github.com/digitalnsw/rules-as-code-template"
	},
	"scripts": {
		"vue:dev": "vue-cli-service serve",
		"vue:build": "vue-cli-service build",
		"vue:lint": "vue-cli-service lint && npm run build"
	},
	"license": "MIT",
	"eslintConfig": {
		"root": true,
		"env": {
			"node": true
		},
		"extends": [
			"plugin:vue/essential",
			"eslint:recommended"
		],
		"rules": {},
		"parserOptions": {
			"parser": "babel-eslint"
		}
	},
	"devDependencies": {
		"@vue/cli-plugin-babel": "^3.9.2",
		"@vue/cli-plugin-eslint": "^3.9.2",
		"@vue/cli-service": "^3.9.2",
		"babel-eslint": "^10.0.2",
		"browser-sync": "^2.26.7",
		"eslint": "^5.16.0",
		"eslint-plugin-vue": "^5.2.3",
		"onchange": "^4.1.0",
		"vue-loader": "^15.7.0",
		"vue-template-compiler": "^2.6.10"
	},
	"dependencies": {
		"@digital.nsw/core": "latest",
		"@digital.nsw/header": "latest",
		"@gov.au/accordion": "latest",
		"@gov.au/animate": "latest",
		"@gov.au/body": "latest",
		"@gov.au/breadcrumbs": "latest",
		"@gov.au/buttons": "latest",
		"@gov.au/callout": "latest",
		"@gov.au/control-input": "latest",
		"@gov.au/cta-link": "latest",
		"@gov.au/direction-links": "^3.0.0",
		"@gov.au/footer": "latest",
		"@gov.au/grid-12": "latest",
		"@gov.au/headings": "latest",
		"@gov.au/inpage-nav": "latest",
		"@gov.au/keyword-list": "latest",
		"@gov.au/link-list": "latest",
		"@gov.au/main-nav": "latest",
		"@gov.au/page-alerts": "latest",
		"@gov.au/pancake-sass": "^2.3.7",
		"@gov.au/progress-indicator": "latest",
		"@gov.au/responsive-media": "latest",
		"@gov.au/select": "latest",
		"@gov.au/side-nav": "latest",
		"@gov.au/skip-link": "latest",
		"@gov.au/tags": "latest",
		"@gov.au/text-inputs": "latest",
		"autoprefixer": "^9.6.1",
		"axios": "^0.18.1",
		"node-sass": "^4.9.2",
		"postcss-cli": "^6.1.3",
		"vue": "^2.6.10",
		"vue-router": "^3.0.7",
		"vuex": "^3.1.1"
	},
	"pancake": {
		"auto-save": true,
		"plugins": true,
		"ignore": [
			"@gov.au/pancake-react",
			"@gov.au/pancake-json"
		],
		"css": {
			"minified": true,
			"modules": false,
			"browsers": [
				"last 2 versions",
				"ie 8",
				"ie 9",
				"ie 10"
			],
			"location": false,
			"name": false
		},
		"org": "@digital.nsw @gov.au",
		"sass": {
			"modules": false,
			"location": "src/sass/",
			"name": "_auds.scss"
		},
		"js": {
			"minified": true,
			"modules": false,
			"location": "src/js/",
			"name": "main.js"
		}
	},
	"browserslist": [
		"last 2 versions",
		"ie 8",
		"ie 9",
		"ie 10"
	]
}
